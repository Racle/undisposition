{
  "author": "@racle",
  "name": "Undisposition [Racle fork]",
  "description": "Removes Content-Disposition: attachment HTTP header to allow view files instead of downloading them. Extension icon acts as toggle.",
  "version": "0.0.5",
  "manifest_version": 3,
  "background": {
    "service_worker": "undisposition_bg.js",
    "type": "module"
  },
  "icons": {
    "16": "icons/icon16.png",
    "19": "icons/icon19.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_title": "Undisposition"
  },
  "permissions": ["contextMenus", "webRequest", "declarativeNetRequest", "declarativeNetRequestWithHostAccess", "declarativeNetRequestFeedback", "storage"],
  "host_permissions": ["http://*/", "https://*/"]
}
